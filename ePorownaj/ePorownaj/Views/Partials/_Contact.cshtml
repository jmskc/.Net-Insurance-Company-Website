@inherits UmbracoViewPage
<div data-aos="fade-up" class="highlight-phone" style="background-image:url(&quot;/assets/img/call.png&quot;);">
    <div class="container">
        <div class="row">
            <div class="col-sm-8">
                <div class="intro">
                    <h2>@Umbraco.Field("kontaktTytul", recursive: true)</h2>
                    <p>Zadzwoń pod @Umbraco.Field("numerTelefonu", recursive: true) aby uzyskać więcej informacji, bądź skorzystaj z naszego formularza kontaktowego online poniżej.</p><a class="btn btn-primary" role="button" href="/kontakt">Kontakt</a>
                </div>
            </div>
            <div class="col-sm-4 hidden-xs">
                <div class="iphone-mockup"></div>
            </div>
        </div>
    </div>
</div>
